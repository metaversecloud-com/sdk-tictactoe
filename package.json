{
  "name": "rnd-boilerplate",
  "private": true,
  "author": "Automatipia-MC",
  "version": "1.0.0",
  "description": "This boilerplate is for the RND projects.",
  "main": "index.js",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start:dev": "concurrently \"npm run serve --workspace=server\" \"npm run dev --workspace=client\"",
    "build:prod": "npm run build -w server && npm run build -w client"
  },
